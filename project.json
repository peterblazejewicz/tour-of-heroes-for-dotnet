    "Microsoft.Extensions.SecretManager.Tools": {
      "version": "1.0.0-preview1-final",
      "imports": "portable-net45+win8+dnxcore50"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "compile": {
      "exclude": [
        "config",
        "dist",
        "e2e",
        "node_modules",
        "public",
        "src",
        "tmp",
        "typings"
      ]
    }
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "publishOptions": {
    "include": [
      "dist",
      "appsettings.json",
      "appsettings.Production.json",
      "hosting.json",
      "web.config"
    ],
    "exclude": [
      "config",
      "e2e",
      "src",
      "tmp",
      "typings"
    ],
    "excludeFiles": [
      ".editorconfig",
      ".gitignore",
      "angular-cli.json",
      "angular-cli-build.js",
      "package.json",
      "tsling.json",
      "typings.json"
    ]
  },
  "scripts": {
    "precompile": [
      "ng build"
    ],
    "prepublish": [
      "npm install",
      "bower install"
    ],
    "postpublish": [
      "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%"
    ]
  },
  "tooling": {
    "defaultNamespace": "TourOfHeroes"
